{
  "name": "smooth-swipeable",
  "version": "0.0.1",
  "description": "Swipe React HOC with RxJS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bill42362/smooth-swipeable"
  },
  "main": "dist/js/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "buildclient": "NODE_OPTIONS='--max-old-space-size=8192' webpack --config webpack/client.babel.js",
    "buildhtml": "babel-node src/server/buildHtml.js",
    "build": "npm run buildclient && npm run buildhtml",
    "buildstatic": "rimraf dist/client && NODE_ENV=production npm run build",
    "mkcert": "mkcert -cert-file devserver.crt -key-file devserver.key localhost.bill42362.net",
    "start": "npm run mkcert && babel-node --max-old-space-size=8192 src/server",
    "prettier": "prettier -l --write '**/*.js' '**/*.jsx' '**/*.json' --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [
    "React",
    "RxJS"
  ],
  "author": "Pi-Yin Hsiao",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.2.0",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "re-reselect": "^3.4.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-ga": "^2.6.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.2",
    "styled-components": "^4.3.2",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-assets": "^1.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "happypack": "^5.0.1",
    "helmet": "^3.20.0",
    "html-minifier": "^4.0.0",
    "imagemin": "^7.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "img-loader": "^3.0.1",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.10",
    "rimraf": "^3.0.0",
    "stats-webpack-plugin": "^0.7.0",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-flush-chunks": "^2.0.3",
    "webpack-hot-middleware": "^2.25.0"
  }
}
